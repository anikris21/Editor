            {
                "scope": ["entity.name.function.jsx", "keyword.control.flow.js" ,"keyword.control.loop.js" , "keyword.control.conditional.js", "keyword.control.trycatch.js", "keyword.control.at-rule.media.css" ],
                "settings": {
